{
  "name": "larapacks/setting",
  "description": "Persistent Laravel configuration settings.",
  "keywords": ["setting", "settings", "configuration"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "illuminate/database": "~5.2|~5.3|~5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "orchestra/testbench": "~3.2"
  },
  "autoload": {
    "files": [
      "src/helper.php"
    ],
    "psr-4": {
      "Larapacks\\Setting\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapacks\\Setting\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
